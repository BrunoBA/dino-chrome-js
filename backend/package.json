{
  "name": "dino-chrome-js",
  "version": "1.0.0",
  "description": "A simple project using TS and Html Canvas",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/entry.ts\" -e ts",
    "server": "lite-server -c browser-sync.json",
    "watch": "tsc -w",
    "start": "concurrently \"npm run watch\" \"npm run server\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "lite-server": "^2.5.4"
  }
}
